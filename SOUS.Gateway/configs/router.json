{
  "router": {
    "routes": [
      {
        "clusterId": "testcluster",
        "mask": "/api/service1/v1/*rest",
        "methods": ["GET"]
      },
      {
        "clusterId": "testcluster2",
        "mask": "/api/service1/v2/*rest",
        "methods": ["GET"]
      }
    ],
    "clusters": {
      "testcluster": {
        "destinations": [
          {
            "url": "http://localhost:8080/",
            "weight": 2
          },
          {
            "url": "http://localhost:8081/"
          }
        ],
        "loadBalancer": "IPHash"
      },
      "testcluster2": {
        "destinations": [
          {
            "url": "http://localhost:8080/"
          },
          {
            "url": "http://localhost:8081/"
          }
        ],
        "loadBalancer": "Random"
      }
    }
  }
}